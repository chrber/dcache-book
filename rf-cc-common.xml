<section id="rf-cc-common">
  <title>Common Cell Commands</title>
  
  <refentry id="cmd-pin">        
    <refmeta><refentrytitle><command>pin</command></refentrytitle></refmeta>
    <refnamediv><refname><command>pin</command></refname>
      <refpurpose>Adds a comment to the pinboard.</refpurpose>
    </refnamediv>
    
    <refsynopsisdiv>
      <cmdsynopsis>
        <command>pin</command>
        <arg choice="req">
          <replaceable>comment</replaceable>
        </arg>
      </cmdsynopsis>
      <variablelist>
        <title>Arguments</title>
        <varlistentry>
          <term>comment</term>
          <listitem>
            <para>
              A string which is added to the pinboard.
            </para>
          </listitem>
        </varlistentry>
      </variablelist>
    </refsynopsisdiv>

    <refsection>
      <title>Description</title>
      
      <para>
      </para>
      
    </refsection>
  </refentry>
  
  
  <refentry id="cmd-info">        
    <refmeta><refentrytitle><command>info</command></refentrytitle></refmeta>
    <refnamediv><refname><command>info</command></refname>
      <refpurpose>Print info about the cell.</refpurpose>
    </refnamediv>
    
    <refsynopsisdiv>
      <cmdsynopsis>
        <command>info</command>
        <arg>-a</arg>
        <arg>-l</arg>
      </cmdsynopsis>
      <variablelist>
        <title>Arguments</title>
        <varlistentry>
          <term>-a</term>
          <listitem>
            <para>
              Display more information.
            </para>
          </listitem>
        </varlistentry>
        <varlistentry>
          <term>-l</term>
          <listitem>
            <para>
              Display long information.
            </para>
          </listitem>
        </varlistentry>
      </variablelist>
    </refsynopsisdiv>
    
    <refsection>
      <title>Description</title>
      
      <para>
        The info printed by <command>info</command> depends on the cell class.
      </para>
      
    </refsection>
  </refentry>
  
